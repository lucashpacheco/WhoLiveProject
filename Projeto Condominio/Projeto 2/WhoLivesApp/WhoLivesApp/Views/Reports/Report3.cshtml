@model IEnumerable<WhoLivesApp.Models.Report3>

@{
    ViewBag.Title = "Report3";
}

<h2>Veiculos por apartamentos</h2>


<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.Apartamento)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Veiculos)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.Apartamento)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Veiculos)
        </td>
       
    </tr>
}

</table>

<p>
    @Html.ActionLink("Voltar para relatórios", "Reports", "Home")
</p>
