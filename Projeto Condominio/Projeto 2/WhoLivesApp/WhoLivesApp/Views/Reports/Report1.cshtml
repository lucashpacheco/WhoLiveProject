@model IEnumerable<WhoLivesApp.Models.Report1>

@{
    ViewBag.Title = "Report1";
}

<h2>Relatorio - Moradores por apartamento</h2>


<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.AP_NUMERO)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.MOR_NOME)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.MOR_SOBRENOME)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.AP_ATENCAO)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.FUN_NOME)
        </th>
        <th></th>
    </tr>

    @foreach (var item in Model)
    {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.AP_NUMERO)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.MOR_NOME)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.MOR_SOBRENOME)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.AP_ATENCAO)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.FUN_NOME)
            </td>
           
        </tr>


    }

</table>
<p>
    @Html.ActionLink("Voltar pra relatórios", "Reports", "Home")
</p>